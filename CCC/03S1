#include <iostream>
#include <cstring>
#include <algorithm>
#include <cmath>

using namespace std;


int main()
{
    int n, cnt = 1;
    while(cin>>n)
    {
        if(n == 0)
        {
            cout<<"You Quit!"<<endl;
            break;
        }
        else if((cnt + n) > 100)
        {
            cout<<"You are now on square "<<cnt<<endl;
        }
        else if((cnt + n) == 54)
        {
            cout<<"You are now on square "<<19<<endl;
            cnt = 19;
        }
        else if((cnt + n) == 90)
        {
            cout<<"You are now on square "<<48<<endl;
            cnt = 48;
        }
        else if((cnt + n) == 99)
        {
            cout<<"You are now on square "<<77<<endl;
            cnt = 77;
        }
        else if((cnt + n) == 9)
        {
            cout<<"You are now on square "<<34<<endl;
            cnt = 34;
        }
        else if((cnt + n) == 40)
        {
            cout<<"You are now on square "<<64<<endl;
            cnt = 64;
        }
        else if((cnt + n) == 67)
        {
            cout<<"You are now on square "<<86<<endl;
            cnt = 86;
        }
        else if((cnt + n) == 100)
        {
            cout<<"You are now on square "<<100<<endl;
            cout<<"You Win!"<<endl;
            break;
        }
        else
        {
            cnt += n;
            cout<<"You are now on square "<<cnt<<endl;
        }
    }
    
    
    
    return 0;
}
